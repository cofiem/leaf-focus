{
  "process": {
    "broker": "amqp://{{ rabbitmq_url }}:{{ port_rabbitmq }}",
    "backend": "redis://localhost:{{ port_redis }}/0",
    "default-queue": "{{ celery_queue_pdf }}",
    "task-queues": {
      "leaf-focus.store.identify": {
        "queue": "{{ celery_queue_pdf }}"
      },
      "leaf-focus.pdf.images": {
        "queue": "{{ celery_queue_pdf }}"
      },
      "leaf-focus.pdf.info": {
        "queue": "{{ celery_queue_pdf }}"
      },
      "leaf-focus.pdf.text": {
        "queue": "{{ celery_queue_pdf }}"
      },
      "leaf-focus.ocr.prepare": {
        "queue": "{{ celery_queue_ocr_prepare }}"
      },
      "leaf-focus.ocr.recognise": {
        "queue": "{{ celery_queue_ocr_recognise }}"
      }
    }
  },
  "exe": {
    "pdf-images": "{{ xpdf_images }}",
    "pdf-info": "{{ xpdf_info }}",
    "pdf-text": "{{ xpdf_text }}"
  },
  "ocr": {
    "threshold": 190
  },
  "pdf": {
    "data-dir": "{{ app_data_dir }}",
    "items-dir": "{{ app_items_dir }}",
    "cache-dir": "{{ app_cache_dir }}",
    "allowed-domains": [
      "aph.gov.au"
    ],
    "input-urls": [
      {
        "category": "members",
        "url": "https://www.aph.gov.au/Senators_and_Members/Members/Register",
